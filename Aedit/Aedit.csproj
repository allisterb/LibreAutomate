<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net5.0</TargetFramework>
		<UseWPF>true</UseWPF>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>..\Au.snk</AssemblyOriginatorKeyFile>
		<CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
		<OutDir>$(SolutionDir)_</OutDir>
		<NoWarn>1701;1702;3001,3002,3003,3009;169;649;</NoWarn>
		<LangVersion>preview</LangVersion>
		<NoWin32Manifest>true</NoWin32Manifest>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
	  <DefineConstants>DEBUG;TRACE</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="Default\Commands.xml" />
		<None Remove="Default\Layout.xml" />
	</ItemGroup>

	<ItemGroup>
	  <Page Remove="Resources\Images\BuildSelection_16x.xaml" />
	  <Page Remove="Resources\Images\Copy_16x.xaml" />
	  <Page Remove="Resources\Images\Cut_16x.xaml" />
	  <Page Remove="Resources\Images\FindInFile_16x.xaml" />
	  <Page Remove="Resources\Images\Image_16x.xaml" />
	  <Page Remove="Resources\Images\Paste_16x.xaml" />
	  <Page Remove="Resources\Images\Redo_16x.xaml" />
	  <Page Remove="Resources\Images\SaveAll_16x.xaml" />
	  <Page Remove="Resources\Images\SettingsGroup_16x.xaml" />
	  <Page Remove="Resources\Images\StartWithoutDebug_16x.xaml" />
	  <Page Remove="Resources\Images\StatusHelp_16x.xaml" />
	  <Page Remove="Resources\Images\Stop_16x.xaml" />
	  <Page Remove="Resources\Images\test.xaml" />
	  <Page Remove="Resources\Images\Undo_16x.xaml" />
	  <Page Remove="Resources\Images\WordWrap_16x.xaml" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="Default\Commands.xml">
		  <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
		<Content Include="Default\Layout.xml">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<SubType>Designer</SubType>
		</Content>
	</ItemGroup>

	<ItemGroup>
	  <Resource Include="Resources\Images\test.xaml" />
	  <Resource Include="Resources\Images\BuildSelection_16x.xaml" />
	  <Resource Include="Resources\Images\Copy_16x.xaml" />
	  <Resource Include="Resources\Images\Cut_16x.xaml" />
	  <Resource Include="Resources\Images\FindInFile_16x.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </Resource>
	  <Resource Include="Resources\Images\Image_16x.xaml" />
	  <Resource Include="Resources\Images\Paste_16x.xaml" />
	  <Resource Include="Resources\Images\Redo_16x.xaml" />
	  <Resource Include="Resources\Images\SaveAll_16x.xaml" />
	  <Resource Include="Resources\Images\SettingsGroup_16x.xaml" />
	  <Resource Include="Resources\Images\StartWithoutDebug_16x.xaml" />
	  <Resource Include="Resources\Images\StatusHelp_16x.xaml" />
	  <Resource Include="Resources\Images\Stop_16x.xaml" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Au.Controls\Au.Controls.csproj" />
	  <ProjectReference Include="..\Au\Au.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Resource Include="Resources\Images\NewFile_16x.xaml" />
	  <Resource Include="Resources\Images\Property_16x.xaml" />
	  <Resource Include="Resources\Images\Undo_16x.xaml" />
	  <Resource Include="Resources\Images\WordWrap_16x.xaml" />
	</ItemGroup>

	<Target Name="PreBuild" BeforeTargets="PreBuildEvent">
		<Exec Command="&quot;$(SolutionDir)Other\Programs\nircmd.exe&quot; win close etitle Aedit" />
	</Target>

	<Target Name="PostBuild" AfterTargets="PostBuildEvent">
		<Exec Command="cd $(TargetDir)&#xD;&#xA;del &quot;$(TargetName).exe&quot;&#xD;&#xA;&quot;$(SolutionDir)Other\Programs\ResourceHacker.exe&quot; -script &quot;$(ProjectDir)Resources\hacker.txt&quot;&#xD;&#xA;::&quot;$(SolutionDir)Other\Programs\ResourceHacker.exe&quot; -script &quot;$(ProjectDir)Resources\hacker32.txt&quot;&#xD;&#xA;del &quot;$(TargetName).*.json&quot;&#xD;&#xA;" />
	</Target>

	<ProjectExtensions><VisualStudio><UserProperties default_4commands_1json__JsonSchema="" /></VisualStudio></ProjectExtensions>

</Project>
