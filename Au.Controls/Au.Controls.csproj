<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net5.0-windows</TargetFramework>
		<UseWPF>true</UseWPF>
		<UseWindowsForms>true</UseWindowsForms>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<AssemblyName>Au.Controls</AssemblyName>
		<RootNamespace>Au.Controls</RootNamespace>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>..\Au.snk</AssemblyOriginatorKeyFile>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<DocumentationFile>Au.Controls.xml</DocumentationFile>
		<NoWarn>1701;1702;1591;0419</NoWarn>
		<LangVersion>preview</LangVersion>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DefineConstants>DEBUG;TRACE</DefineConstants>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="Toolbars\**" />
		<EmbeddedResource Remove="Toolbars\**" />
		<None Remove="Toolbars\**" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Remove="Forms\excluded\ComboBox_.cs" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Libraries\_SourceGrid\_SourceGrid.csproj" />
		<ProjectReference Include="..\Libraries\_TreeList\_TreeList.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Forms\AuDockPanel\AuDockPanel.cs" />
		<Compile Update="Forms\AuDockPanel\DP common.cs" />
		<Compile Update="Forms\AuDockPanel\_Acc.cs" />
		<Compile Update="Forms\AuDockPanel\_Float.cs" />
		<Compile Update="Forms\AuDockPanel\_Panel.cs" />
		<Compile Update="Forms\AuDockPanel\_Split.cs" />
		<Compile Update="Forms\AuDockPanel\_Tab.cs" />
		<Compile Update="Forms\AuStripManager\FSMProperties.cs" />
		<Compile Update="Forms\Other\DialogForm.cs" />
		<Compile Update="Forms\Other\AuUserControlBase.cs" />
		<Compile Update="Forms\Other\InactiveWindow.cs" />
		<Compile Update="Forms\Other\ParamGrid.cs" />
		<Compile Update="Forms\Other\Simple.cs" />
		<Compile Update="Forms\Other\ToolStripSpringBox.cs" />
		<Compile Update="Forms\Sci\AuScintilla.cs" />
		<Compile Update="Forms\Sci\InfoBox.cs" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Simple\" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="Forms\excluded\ComboBox_.cs">
	    <SubType>Component</SubType>
	  </None>
	</ItemGroup>

	<Target Name="PreBuild" BeforeTargets="PreBuildEvent">
		<Exec Command="&quot;$(SolutionDir)Other\Programs\nircmd.exe&quot; win close etitle Aedit&#xD;&#xA;&quot;$(SolutionDir)Other\Programs\nircmd.exe&quot; win close etitle QM3" />
	</Target>

</Project>
