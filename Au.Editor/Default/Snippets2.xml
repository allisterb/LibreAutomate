<Au.Snippets>

	<!--
	Edit this file to add more snippets. The code editor adds these + default snippets to completion lists.
	Location of this file: folders.ThisAppDocuments + @".settings\Snippets.xml".
	Location of file with default snippets: folders.ThisApp + @"Default\Snippets.xml". Don't edit.
	In snippet code can be used $end$ to set final text cursor position. Or $end$select$end$.
	Examples:

	<group context="Function">
		<snippet name="winFindSnippet" info="Find window." more="With non-negative waitS argument (0, ) throws exception if not found.">
			<![CDATA[var w = wnd.find(0, "$end$*Name$end$");]]>
		</snippet>

		<snippet name="foriSnippet" info="Repeat count times. Set variable i = 0 and increment after each loop.">
			<![CDATA[for (int i = 0; i < $end$count$end$; i++) {
	
}]]>
		</snippet>

		<snippet name="dsDialogShowSnippet" info="Standard dialog window.">
			<list item="OK button">dialog.show("$end$");</list>
			<list item="OK button and info icon">dialog.showInfo("$end$");</list>
		</snippet>

	</group>
	
	<group context="Function|Arrow">
		<snippet name="piSnippet" info="Display text and variables in the output window.">
			<![CDATA[print.it($end$);]]>
		</snippet>

	</group>
	-->
	
	

</Au.Snippets>